---
permalink: /feed.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:media="http://search.yahoo.com/mrss/">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>https://dai-global-developments.com</link>
    <atom:link href="{{ "https://dai-global-developments.com/feed.xml"}}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ page.date | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ page.date | date_to_rfc822 }}</lastBuildDate>
    <generator>Eleventy</generator>
    {% for post in collections.justPosts limt:10 %}
      <item>
        <media:content url="https://dai-global-developments.com{{ post.data.image }}" medium="image" />
        <title>{{ post.data.title | xml_escape }}</title>
        <description>{{ post.data.excerpt | xml_escape | replace:'&lt;p&gt;&lt;img src=&quot;','&lt;p&gt;&lt;img src=&quot;https://dai-global-digital.com' }}</description>
        <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
        <link>https://dai-global-developments.com{{ post.url }}</link>
        <guid isPermaLink="true">https://dai-global-developments.com{{ post.url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>